# Uncomment the next line to define a global platform for your project
platform :ios, '15.0'

use_frameworks!

# Pods for all Targets
def shared_pods
    pod 'Alamofire'
    pod 'SDWebImage', '~> 5.11'
    pod 'SwiftLint'
end

target 'ThePugApp' do
    shared_pods
end

target 'ThePugAppTests' do
    shared_pods
end
